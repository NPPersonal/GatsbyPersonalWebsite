{
  "name": "npwebsite",
  "version": "1.0.0",
  "private": true,
  "description": "npwebsite",
  "author": "tomneo2004",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cbs": "gatsby clean && gatsby build && gatsby serve",
    "extract": "babel --verbose --config-file ./babel-extract.config.js -o tmp/chunk.js src/**/*.{js,jsx,ts,tsx} && rm -r tmp",
    "babelhelp": "babel --help"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.14.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fec/remark-a11y-emoji": "^4.0.2",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.18",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.0.3",
    "autoprefixer": "^10.4.15",
    "clsx": "^2.0.0",
    "formik": "^2.4.5",
    "gatsby": "^5.12.1",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-image": "^3.12.0",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-mdx": "^5.12.3",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-postcss": "^6.12.0",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-sharp": "^5.12.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-sharp": "^5.12.0",
    "i18next": "^22.5.1",
    "postcss": "^8.4.28",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-accessible-treeview": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-intersection-observer": "^9.8.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-loading": "^2.0.3",
    "react-markdown": "^9.0.1",
    "react-material-ui-carousel": "^3.4.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/github": "^9.0.4",
    "babel-plugin-i18next-extract": "^0.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^21.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github"
  ]
}
